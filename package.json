{
  "name": "child-care-online-services",
  "version": "1.0.0",
  "description": "App uses the latest technology from 2016 , react for view layer , redux for data flow , react router for navigation , redux thunk for asyn data fetching , node & mongo on the server seide. ",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && NODE_ENV=production node server.bundle.js",
    "build:client": "webpack --progress --colors",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "build": "npm run build:client && npm run build:server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.1",
    "client-sessions": "^0.7.0",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.0",
    "mongoose": "^4.4.20",
    "morgan": "^1.7.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
